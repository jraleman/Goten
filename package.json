{
  "name": "Basico",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "node ./node_modules/standardx/bin/cmd.js --verbose './App/**/*.js' | node ./node_modules/snazzy/bin/cmd.js || true",
    "lint-fix": "node ./node_modules/standardx/bin/cmd.js --parser babel-eslint --fix --verbose './App/**/*.js' | node ./node_modules/snazzy/bin/cmd.js || true",
    "lint-log": "npm run lint > lint.log",
    "doc": "./node_modules/.bin/jsdoc --verbose -r ./App || true",
    "surrender": "node ./node_modules/react-native-surrender/index.js",
    "upgrade": "node ./node_modules/react-native-git-upgrade/index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.0.0",
    "native-base": "^2.6.1",
    "react": "16.5.0",
    "react-native": "0.57.1",
    "react-native-app-intro-slider": "^0.2.4",
    "react-native-calendars": "^1.20.0",
    "react-native-i18n": "^2.0.12",
    "react-native-search-filter": "^0.1.4",
    "react-native-star-rating": "^1.1.0",
    "react-native-true-sight": "^1.0.0-alpha4",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.3.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.1.2",
    "babel-eslint": "^8.2.6",
    "babel-jest": "23.6.0",
    "eslint": "^4.19.1",
    "jest": "23.6.0",
    "jsdoc": "~3.5.5",
    "metro-react-native-babel-preset": "0.48.1",
    "react-test-renderer": "16.5.0",
    "snazzy": "^8.0.0",
    "standardx": "^3.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "standardx": {
    "ignore": [
      "App/Themes/variables.js"
    ]
  }
}
